{
  "require": {
    "ext-pdo": "*",
    "doctrine/migrations": "^3.4",
    "illuminate/database": "^9.8",
    "illuminate/events": "^9.8",
    "illuminate/container": "^9.8",
    "twig/twig": "^3.4",
    "twig/intl-extra": "^3.4",
    "vlucas/phpdotenv": "^5.5"

  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
